<div class="row sign-in-form" id="content">
  <div class="medium-3 medium-offset-4 columns">
    <h5>Have an account?</h5>
    <%= form_for @user, url: "/sessions", method: "post" do |f| %>
    <%= f.text_field :username, placeholder:"Username" %> <br>
    <%= f.password_field :password, placeholder:"Password" %> <br>
    <%= f.submit "Sign In", class:"button radius"%>
    <% end %>
  </div>
</div>
